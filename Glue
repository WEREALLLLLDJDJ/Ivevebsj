                hrp.CFrame = CFrame.new(pos + v3.new(0, 10, 0), lookAt)
                hum:MoveTo(finishLine.Position, finishLine)
            end
            while wait() do
                if getgenv().Captain then
                    repeat
                        step:wait()
                    until finishLine.Position.Y ~= 0
                    wait()
                    toFinish()
                    repeat
                        wait()
                    until finishLine.Position.Y == 0
                else
                    wait()
                end
            end
        end
    )
